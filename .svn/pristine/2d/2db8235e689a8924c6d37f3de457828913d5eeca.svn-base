<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.itheima.domain.Customer" table="cst_custoemr">
    	<id name="cust_id" column="cust_id">
    		<generator class="native"></generator>
    	</id>
    	
    	<property name="cust_name" column="cust_name"></property>
    	<property name="cust_source" column="cust_source"></property>
    	<property name="cust_industry" column="cust_industry"></property>
    	<property name="cust_level" column="cust_level"></property>
    	<property name="cust_phone" column="cust_phone"></property>
    	<property name="cust_mobile" column="cust_mobile"></property>
    	
    	<many-to-one name="dictSource" class="com.itheima.domain.BaseDict" column="cust_source"></many-to-one>
    	<many-to-one name="dictIndustry" class="com.itheima.domain.BaseDict" column="cust_industry"></many-to-one>
    	<many-to-one name="dictLevel" class="com.itheima.domain.BaseDict" column="cust_level"></many-to-one>
    
    </class>
    </hibernate-mapping>